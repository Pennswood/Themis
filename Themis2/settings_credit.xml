<settings>
    <name>Software</name>
    <seed>42</seed>
    <max_samples>1000</max_samples>
    <min_samples>3</min_samples>
    <inputs>
        <input>
            <name>checking-account-status</name>
            <type>categorical</type>
            <values>
                <value>owe-money</value>
                <value>0-200DM</value>
                <value>200+DM</value>
                <value>no-account</value>
            </values>
        </input>
        <input>
            <name>duration-months</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>4</lowerbound>
                <upperbound>72</upperbound>
            </bounds>
        </input>
        <input>
            <name>credit-history</name>
            <type>categorical</type>
            <values>
                <value>all-credit-paid-on-time</value>
                <value>cur-ank-all-credit-paid-on-time</value>
                <value>existing-credit-paid-on-time</value>
                <value>day-in-paying-credit</value>
                <value>critical-accounts</value>
            </values>
        </input>
        <input>
            <name>purpose</name>
            <type>categorical</type>
            <values>
                <value>new-car</value>
                <value>used-car</value>
                <value>furniture</value>
                <value>radio-television</value>
                <value>domestic-appliance</value>
                <value>repairs</value>
                <value>education</value>
                <value>vacation</value>
                <value>retraining</value>
                <value>buisness</value>
                <value>others</value>
            </values>
        </input>
        <input>
            <name>credit-amount</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>250</lowerbound>
                <upperbound>18424</upperbound>
            </bounds>
        </input>
        <input>
            <name>saving-account-status</name>
            <type>categorical</type>
            <values>
                <value>&amp;lt;100DM</value>
                <value>100-500DM</value>
                <value>500-1000DM</value>
                <value>1000+DM</value>
                <value>unknown-nosavings</value>
            </values>
        </input>
        <input>
            <name>employment-status</name>
            <type>categorical</type>
            <values>
                <value>unemployed</value>
                <value>&amp;lt;1yr</value>
                <value>1-4yr</value>
                <value>4-7yr</value>
                <value>7+yr</value>
            </values>
        </input>
        <input>
            <name>disposable-income-percent</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>1</lowerbound>
                <upperbound>4</upperbound>
            </bounds>
        </input>
        <input>
            <name>sex</name>
            <type>categorical</type>
            <values>
                <value>male</value>
                <value>female</value>
            </values>
        </input>
        <input>
            <name>other-debts</name>
            <type>categorical</type>
            <values>
                <value>none</value>
                <value>co-applicant</value>
                <value>guarantor</value>
            </values>
        </input>
        <input>
            <name>current-residence-time</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>1</lowerbound>
                <upperbound>4</upperbound>
            </bounds>
        </input>
        <input>
            <name>property-type</name>
            <type>categorical</type>
            <values>
                <value>real-estate</value>
                <value>insurance</value>
                <value>car</value>
                <value>unknown</value>
            </values>
        </input>
        <input>
            <name>age</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>19</lowerbound>
                <upperbound>75</upperbound>
            </bounds>
        </input>
        <input>
            <name>installment-plans</name>
            <type>categorical</type>
            <values>
                <value>bank</value>
                <value>stores</value>
                <value>none</value>
            </values>
        </input>
        <input>
            <name>housing</name>
            <type>categorical</type>
            <values>
                <value>rent</value>
                <value>own</value>
                <value>free</value>
            </values>
        </input>
        <input>
            <name>num-credit-lines</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>1</lowerbound>
                <upperbound>4</upperbound>
            </bounds>
        </input>
        <input>
            <name>job-type</name>
            <type>categorical</type>
            <values>
                <value>unemployed(non-resident)</value>
                <value>unemployed(resident)</value>
                <value>low-skill-employed</value>
                <value>high-skill-employed</value>
            </values>
        </input>
        <input>
            <name>num-dependents</name>
            <type>continuousInt</type>
            <bounds>
                <lowerbound>1</lowerbound>
                <upperbound>2</upperbound>
            </bounds>
        </input>
        <input>
            <name>telephone</name>
            <type>categorical</type>
            <values>
                <value>none</value>
                <value>yes</value>
            </values>
        </input>
        <input>
            <name>foreigner</name>
            <type>categorical</type>
            <values>
                <value>yes</value>
                <value>no</value>
            </values>
        </input>
    </inputs>
</settings>